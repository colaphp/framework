{
  "name": "daophp/framework",
  "description": "The DaoPHP Framework.",
  "keywords": ["framework", "daophp"],
  "license": "MIT",
  "homepage": "https://www.daophp.com",
  "support": {
    "issues": "https://github.com/daophp/framework/issues",
    "source": "https://github.com/daophp/framework"
  },
  "require": {
    "php": "^8.0.2",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "doctrine/annotations": "^1.13",
    "firebase/php-jwt": "^5.5",
    "gregwar/captcha": "^1.1",
    "illuminate/database": "^8.77",
    "illuminate/events": "^8.77",
    "illuminate/pagination": "^8.77",
    "illuminate/redis": "^8.77",
    "intervention/image": "^2.7",
    "jenssegers/blade": "^1.4",
    "monolog/monolog": "^2.3",
    "nikic/fast-route": "^1.3",
    "overtrue/easy-sms": "^2.0",
    "overtrue/pinyin": "^4.0",
    "overtrue/wechat": "^6.0",
    "php-di/php-di": "^6.3",
    "phpoffice/phpspreadsheet": "^1.20",
    "vlucas/phpdotenv": "^5.4",
    "workerman/crontab": "^1.0",
    "workerman/validation": "^3.0",
    "workerman/workerman": "^4.0",
    "yansongda/pay": "^3.0.21"
  },
  "autoload": {
    "psr-4": {
      "Swift\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
