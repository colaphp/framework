{
  "name": "daophp/framework",
  "description": "High performance HTTP Service Framework.",
  "keywords": ["framework", "daophp"],
  "license": "MIT",
  "homepage": "https://www.daophp.com",
  "support": {
    "issues": "https://github.com/daophp/framework/issues",
    "source": "https://github.com/daophp/framework"
  },
  "require": {
    "php": "^8.0.2",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "firebase/php-jwt": "^6.0",
    "gregwar/captcha": "^1.1.9",
    "illuminate/database": "^9.2",
    "illuminate/events": "^9.2",
    "illuminate/pagination": "^9.2",
    "illuminate/redis": "^9.2",
    "illuminate/view": "^9.2",
    "intervention/image": "^3.0",
    "monolog/monolog": "^2.3",
    "nikic/fast-route": "^1.3",
    "vlucas/phpdotenv": "^5.4",
    "workerman/crontab": "^1.0",
    "workerman/redis-queue": "^1.0",
    "workerman/validation": "^3.0",
    "workerman/workerman": "^4.0.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Swift\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
