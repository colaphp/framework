{
  "name": "colasoft/framework",
  "description": "High performance HTTP Service Framework.",
  "keywords": ["framework", "colaphp", "c779e15"],
  "license": "MIT",
  "homepage": "https://www.colaphp.com",
  "support": {
    "issues": "https://github.com/colaphp/framework/issues",
    "source": "https://github.com/colaphp/framework"
  },
  "require": {
    "php": "^8.2",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-redis": "*",
    "ext-sodium": "*",
    "ext-xmlreader": "*",
    "alibabacloud/aliyun-log-php-sdk": "^0.6",
    "aliyuncs/oss-sdk-php": "^2.7",
    "endroid/qr-code": "^5.0",
    "elasticsearch/elasticsearch": "^8.13",
    "filp/whoops": "^2.15",
    "firebase/php-jwt": "^6.10",
    "illuminate/cache": "^11.9",
    "illuminate/database": "^11.9",
    "illuminate/redis": "^11.9",
    "illuminate/pagination": "^11.9",
    "intervention/image": "^3.6",
    "jenssegers/blade": "^2.0",
    "monolog/monolog": "^3.6",
    "nikic/fast-route": "^1.3",
    "overtrue/easy-sms": "^3.0",
    "overtrue/pinyin": "^5.3",
    "overtrue/socialite": "^4.10",
    "php-di/php-di": "^7.0",
    "phpoffice/phpspreadsheet": "^2.1",
    "ramsey/uuid": "^4.7",
    "robmorgan/phinx": "^0.16",
    "symfony/console": "^7.0",
    "symfony/mailer": "^7.0",
    "symfony/messenger": "^7.0",
    "symfony/process": "^7.0",
    "symfony/twig-bridge": "^7.0",
    "vlucas/phpdotenv": "^5.6",
    "w7corp/easywechat": "^6.15",
    "workerman/crontab": "^1.0",
    "workerman/gateway-worker": "^3.1",
    "workerman/redis-queue": "^1.2",
    "yansongda/pay": "^3.7"
  },
  "require-dev": {
    "laravel/pint": "^1.16",
    "phpstan/phpstan": "^1.11",
    "phpunit/phpunit": "^11.2"
  },
  "autoload": {
    "psr-4": {
      "Flame\\": "src/Flame/"
    },
    "files": [
      "src/Flame/Support/helpers.php"
    ]
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
